<nav class="">
    <ul>
        <li> <%= link_to "Store Home", store_path %> </li>
        <% if !!@cart %>
          <li> <%= link_to "Cart", cart_path(current_cart) %></li>
        <% end %>
        <% if !current_user %>
          <li> <%= link_to "Sign in", new_user_session_path, name: "sign in", id: "sign in" %></li>
          <li> <%= link_to "Sign up", new_user_registration_path %></li>
        <% end %>
        <% if !!current_user %>
          <li> <%= link_to "Sign out", destroy_user_session_path, method: :delete, name: "Sign out", id: "Sign out" %></li>
            <% if !!current_cart %>
              <li> <%= link_to "Cart", cart_path(current_cart), name: "Cart", id: "Cart" %></li>
            <% end %>
          <li> Signed in as <%= current_user.email %></li>
        <% end %>
    </ul>
</nav>
